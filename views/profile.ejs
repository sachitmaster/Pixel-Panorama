<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="../stylesheets/profile.css">
    <link rel="stylesheet" href="../stylesheets/cards.css">
    <link rel="stylesheet" href="../stylesheets/bunch.css">
    <link rel="stylesheet" href="../stylesheets/anime.css">
    <link rel="stylesheet" href="../stylesheets/navbar.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
</head>

<body>
    <div class="shadow-title parallax-top-shadow">snap</div>
    <div class="section-parallax parallax-00"
        style="background-image: url('http://www.ivang-design.com/svg-load/parallax/parallax-0.png');"></div>
    <div class="section-parallax parallax-01"
        style="background-image: url('http://www.ivang-design.com/svg-load/parallax/parallax-01.png');"></div>

    <div class="section full-height">
        <div class="section-title-wrap">
            <div class="container">
                <div class="row">
                    <div class="col-12 text-center parallax-fade-top">
                        <p>we do magic</p>
                        <h1>Pixel<br>Panorama</h1>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="section padding-top-bottom over-hide background-dark z-bigger-2">

        <!-- cards -->

        <div class="cards">
            <h1 class="heading">Picture Cards</h1>
            <section class="main">
                <a href="card1">
                    <div class="wrap wrap--1">
                        <div class="container1 container--1">
                            <!-- <p class="para">01. Normal</p> -->
                        </div>
                    </div>
                </a>
                <a href="card2">
                    <div class="wrap wrap--2">
                        <div class="container1 container--2 img-2">
                            <!-- <p>02. Reverse</p> -->
                        </div>
                    </div>
                </a>
                <a href="card3">
                    <div class="wrap wrap--3">
                        <div class="container1 container--3 img-3">
                            <!-- <p>03. Normal</p> -->
                        </div>
                    </div>
                </a>
            </section>
        </div>

        <!-- cards End -->

        <!-- Image Gallery -->

        <div class="middle-header">
            <h1 class="middle-heading">Image Gallery</h1>
        </div>
        <div class="grid">
            <div class="column">
                <img data-enlargable src="https://picsum.photos/500/700?random=1-1" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=1-2" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=1-3" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=1-4" alt="">
            </div>
            <div class="column">
                <img data-enlargable src="https://picsum.photos/500/700?random=2-1" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=2-2" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=2-3" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=2-4" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=2-5" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=2-6" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=2-7" alt="">
            </div>
            <div class="column">
                <img data-enlargable src="https://picsum.photos/500/700?random=3-1" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=3-2" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=3-3" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=3-4" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=3-5" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=3-6" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=3-7" alt="">
            </div>
            <div class="column">
                <img data-enlargable src="https://picsum.photos/500/700?random=4-1" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=4-2" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=4-3" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=4-4" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=4-5" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=4-6" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=4-7" alt="">
            </div>
            <div class="column">
                <img data-enlargable src="https://picsum.photos/500/700?random=5-1" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=5-2" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=5-3" alt="">
                <img data-enlargable src="https://picsum.photos/500/700?random=5-4" alt="">
            </div>
        </div>

        <!-- Image Gallery End -->

        <!-- Profile -->

        <div class="profile-container">
            <h1 class="profile-heading">Profile</h1>
            <div class="profile-card">
                <div class="info">
                    <!-- <h2>Username</h2> -->
                    <h2>
                        <%= user.username %>
                    </h2>
                    <h2>
                        <%= user.email %>
                    </h2>
                </div>
            </div>
        </div>

        <!-- Profile End -->


        <!-- Nav Bar -->

        <div class="container-bar">
            <ul class="nav-bar">
                <a href="/profile" class="route">
                    <li class="nav-item active"><i class="fa-solid fa-house"></i>Home</li>
                </a>
                <a href="/About" class="route">
                    <li class="nav-item"><i class="fa-solid fa-info"></i>About</li>
                </a>
                <a href="/create" class="route">
                    <li class="nav-item"><i class="fa-solid fa-plus"></i>Create</li>
                </a>
                <a href="/logout" class="route">
                    <li class="nav-item"><i class="fa-solid fa-right-from-bracket"></i>Log out</li>
                </a>
            </ul>
        </div>


        <!-- END -->

    </div>


    <!-- Scroll back to top -->

    <div class="scroll-to-top hover-target"></div>

    <!-- Page cursor -->

    <div class='cursor' id="cursor"></div>
    <div class='cursor2' id="cursor2"></div>
    <div class='cursor3' id="cursor3"></div>
</body>

<script src="../javascripts/profile.js"></script>
<script src="../javascripts/cards.js"></script>
<script src="../javascripts/anime.js"></script>
<script src="../javascripts/display.js"></script>

</html>